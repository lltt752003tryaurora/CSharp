    </logger>
  </log4net>
  <appSettings>
    <add key="EnableLogInfo" value="1" />
    <add key="EnableLogSuccess" value="1" />
    <add key="EnableLogError" value="1" />
    <add key="EnableLogException" value="1" />
  </appSettings>
</configuration>